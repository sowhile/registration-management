#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/wdwor/Desktop/Java/project/HospitalRegistration/vue-admin-template-master/node_modules/.store/mkdirp@1.0.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/wdwor/Desktop/Java/project/HospitalRegistration/vue-admin-template-master/node_modules/.store/mkdirp@1.0.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../../mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js" "$@"
fi
